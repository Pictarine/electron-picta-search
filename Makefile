	rm -rf ~/Library/Saved\ Application\ State/com.electron.PictaSearch.savedState/
	rm -rf ~/.dext
	rm -rf ~/Library/Application\ Support/lookr
	rm -rf ~/Library/Application\ Support/PictaSearch

